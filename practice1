import java.util.Scanner;
public class pattern1 {

	public static void main(String[] args) {
		Scanner Sc = new Scanner(System.in);
		
		for(int i=1;i<=5;i++) {
			//System.out.println(" ");
			for(int j=1;j<=5-i;j++) {
				System.out.println(" ");
			
			for(int k=1;k<=i;k++) {
				System.out.print(i);
			}
			}
			System.out.println();
		}
		
}

}
